<mui:ModernWindow x:Class="Migration.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:local="clr-namespace:Migration"
        WindowStartupLocation="CenterScreen"
        Title="Migration Tool"
        Height="800" Width="1200" MinHeight="540" MinWidth="750"
        IsTitleVisible="True"
        ContentSource="/Pages/MainPage.xaml">

    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="Help" Source="/Content/HelpPage.xaml" />
    </mui:ModernWindow.TitleLinks>

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="Welcome to use migration tool.">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="Transform" Source="/Pages/MainPage.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>

</mui:ModernWindow>
